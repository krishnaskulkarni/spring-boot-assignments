public class DemoController {
	
	
	
	@RequestMapping("/hello")
	public String hello() {
		return "hello world..";
	}
	
	@RequestMapping("/add")
	public String getPerson() {
		Person person = new Person(101,"tony");
		return "hello world..";
	}
}
